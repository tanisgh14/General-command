#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() 
{
    //Declare and intialising variables of different data types
    int my_int;
    cin>>my_int;
    float my_float;
    cin>>my_float;
    double my_double;
    cin>>my_double;
    char my_char;
    cin>>my_char;
    string s1 ="hello";
    bool b1=  true;
    cout<<my_int<<endl;
    cout<<my_float<<endl;
    cout<<my_double<<endl;
    cout<<my_char<<endl;
    cout<<s1<<endl;
    cout<<b1<<endl;
    return 0;
}
/*output
2
5.5
2.34
g
2
5.5
2.34
g
hello
1
*/
